{
  "name": "search-insights",
  "description": "Library for reporting click, conversion and view metrics using the Algolia Insights API",
  "version": "2.2.1",
  "engines": {
    "node": ">=8.16.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/search-insights.umd.js",
  "module": "./dist/search-insights.es.js",
  "exports": {
    ".": {
      "import": "./dist/search-insights.es.js",
      "require": "./dist/search-insights.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "release": "shipjs prepare"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/algolia/search-insights.js.git"
  },
  "author": {
    "name": "Algolia",
    "url": "https://www.algolia.com"
  },
  "license": "MIT",
  "dependencies": {},
  "bundlesize": [
    {
      "path": "./dist/search-insights.min.js",
      "maxSize": "2.75 kB"
    }
  ],
  "devDependencies": {
    "typescript": "4.5.4",
    "vite": "2.7.2"
  }
}
